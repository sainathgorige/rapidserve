$TTL    604800
@       IN      SOA     ns.{{ domain_name }}. admin.{{ domain_name }}. (
                          4         ; Serial
                          604800    ; Refresh
                          86400     ; Retry
                          2419200   ; Expire
                          604800 )  ; Negative Cache TTL
;
        IN      NS      ns.{{ domain_name }}.
ns      IN      A       192.168.100.1
@       IN      A       {{ ns_domain_ip }}
